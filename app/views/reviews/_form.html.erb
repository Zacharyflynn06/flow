<%= form_for review do |f| %>
    <%= f.label :content %>
    <%= f.text_area :content %>
    <%= f.label :rating %>
    <%= f.select :rating, options_for_select([1,2,3,4,5]), :include_blank => "please select" %>
    <%= f.hidden_field (:student_id) %>
    <%= f.hidden_field(:course_id) %>
    <%= f.submit %>
<% end %>