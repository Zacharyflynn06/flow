<div class="wrapper">
<h1>Community</h1>

    <h3>Meet our Teachers:</h3>
    <% @users.teacher.each do |user| %>

        <div class="contents">
           <h3> <%= link_to user.full_name, user_path(user) %> </h3>
           <ul>
                <li><%= user.bio %></li>
                <li>Years Teaching: <%= user.years_experience %></li>
           </ul>
        </div>
    <% end %>

    <h3>Students:</h3>
    <% @users.student.each do |user| %>

        <div class="contents">
           <h3> <%= link_to user.full_name, user_path(user) %> </h3>
           <%= user.bio %>
        </div>

    <% end %>

</div>
