<%= render partial: 'application/errors', locals: {object: @user} %>
<div class="wrapper">

    <h1>Sign Up</h1>
        <%= form_for @user do |f| %>
            <%= render partial: 'signup_form', locals: {f: f} %>
    
                <li><%= f.label :password %></li>
                <li><%= f.password_field :password %></li>
                <li><%= f.label :confirm %></li>
                <li><%= f.password_field :password_confirmation %></li>
            </ul>
            <%= f.submit 'Sign Up', class: 'button' %>
    
        <% end %>
        
        <%= button_to "Sign up with Google", '/auth/google_oauth2', class: "button"  %>
</div>

