<%= render partial: 'application/errors', locals: {object: @user} %>

<div class="wrapper">
    <h1 class="heading">Edit User</h1>
    <%= form_for @user do |f| %>
        <%= render partial: 'signup_form', locals: {f: f} %>
        <%= f.submit 'Save Changes', class: 'button' %>
    <% end %>
</div>