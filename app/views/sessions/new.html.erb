<%= render partial: 'application/errors', locals: {object: @user} %>


<h1>Login</h1>

<div>
    <%= form_tag '/login' do %>
       <br>
       <%= label_tag :username %>
       <%= text_field_tag :username %>
       <br>
       <%= label_tag :password%>
       <%= password_field_tag :password%>
       <br>
       <%= submit_tag "Login"%>
    <%end%>
</div>
