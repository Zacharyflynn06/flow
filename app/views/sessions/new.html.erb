<div class='errors'>
    <%= @errors %>
</div>



<h1 class="heading">Log In</h1>

<div class="wrapper">
    <%= form_tag '/login' do %>
       <br>
       <%= label_tag :username %>
       <%= text_field_tag :username %>
       <br>
       <%= label_tag :password%>
       <%= password_field_tag :password%>
       <br>
       <%= submit_tag "Login"%>
    <%end%>
</div>
